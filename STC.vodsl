	dtype curve2Type extends stcBaseType /* utype=stc:stcBaseType*/
	"A curve in 2-D space, defined by its end points and a shape attribute (default: line or great circle)</"
	{

		P1 : double2Type /* utype=stc:double2Type*/ @? "" ;
		P2 : double2Type /* utype=stc:double2Type*/ @? "" ;
		curve_shape : string /* utype=ivoa:string*/ @? "" ;
	}

	dtype double2Type extends stcBaseType /* utype=stc:stcBaseType*/ "A vector of 2 doubles; components are now separated"
	{

		C1 : double1Type /* utype=stc:double1Type*/ @? "" ;
		C2 : double1Type /* utype=stc:double1Type*/ @? "" ;
		unit : posUnitType /* utype=stc:posUnitType*/ @? "" ;
		vel_time_unit : velTimeUnitType /* utype=stc:velTimeUnitType*/ @? "" ;
		gen_unit : string /* utype=ivoa:string*/ @? "" ;
	}

	dtype posAngleType ""
	{

		value : real /* utype=ivoa:real*/ "" ;
		unit : angleUnitType /* utype=stc:angleUnitType*/ "" ;
		reference : posAngleReferenceType /* utype=stc:posAngleReferenceType*/ "" ;
