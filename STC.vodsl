	{

		M11 : double1Type /* utype=stc:double1Type*/ @? "" ;
		M12 : double1Type /* utype=stc:double1Type*/ @? "" ;
		M13 : double1Type /* utype=stc:double1Type*/ @? "" ;
		M21 : double1Type /* utype=stc:double1Type*/ @? "" ;
		M22 : double1Type /* utype=stc:double1Type*/ @? "" ;
		M23 : double1Type /* utype=stc:double1Type*/ @? "" ;
		M31 : double1Type /* utype=stc:double1Type*/ @? "" ;
		M32 : double1Type /* utype=stc:double1Type*/ @? "" ;
		M33 : double1Type /* utype=stc:double1Type*/ @? "" ;
		unit : posUnitType /* utype=stc:posUnitType*/ @? "" ;
		vel_time_unit : velTimeUnitType /* utype=stc:velTimeUnitType*/ @? "" ;
