	}

	dtype double4Type extends stcBaseType /* utype=stc:stcBaseType*/ "A vector of 4 doubles (2x2 matrix)"
	{

		M11 : double1Type /* utype=stc:double1Type*/ @? "" ;
		M12 : double1Type /* utype=stc:double1Type*/ @? "" ;
		M21 : double1Type /* utype=stc:double1Type*/ @? "" ;
