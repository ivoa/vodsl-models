		PosAngle2 : posAngleType /* utype=stc:posAngleType*/ @?
		"Consists of a Size (2 doubles) and optional position angle element" ;
	}

	dtype size2Type extends double2Type /* utype=stc:double2Type*/ ""
	{

		PosAngle : posAngleType /* utype=stc:posAngleType*/ @?
