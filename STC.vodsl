model stc ( 1.33 ) "This is the data model for describing Arnold Rots' STC world."

	include "IVOA.vodsl"

	otype astroSTCDescriptionType
	"Type for generic astronomical STC metadata, in particular Resource Profile, Search Location, Catalog Entry."
	{
	}

	otype fitsType ""
	{

		fitsFileURL : anyURI /* utype=ivoa:anyURI*/ "" ;
		hdu_num : nonnegativeInteger /* utype=ivoa:nonnegativeInteger*/ @? "" ;
		hdu_name : string /* utype=ivoa:string*/ @? "Specifies a FITS file and optionally a specific HDU by HDU number or HDU name

NB Really representys a reference to (an HDU in) a FITS file
currently modelled as a Class, as this represents a FITS file, though possibly only  HDU in a FTS file.
SHould add that structure explicitly in here and use VO-URP's referencing mechanism" ;
	}

	otype coordFITSColumnsType ""
	{

		Name : string /* utype=ivoa:string*/ @? "" ;
		Value : string /* utype=ivoa:string*/ @?
		"The column name for the coordinate value; comma-separated if multi-dimensional" ;
		Error : string /* utype=ivoa:string*/ @?
		"The column name for the coordinate error; comma-separated if multi-dimensional" ;
		Resolution : string /* utype=ivoa:string*/ @? "The column name for the coordinate resolution; comma-separated if multi-dimensional.

TBD SHOULD have multiplicity 1" ;
		Size : string /* utype=ivoa:string*/ @? "The column name for the coordinate size; comma-separated if multi-dimensional.

TBD SHOULD have multiplicity 1" ;
		PixSize : string /* utype=ivoa:string*/ @? "The column name for the coordinate pixel size; comma-separated if multi-dimensional.

TBD SHOULD have multiplicity 1" ;
	}

	otype regionType "Abstract region type; a Region is a Shape or the result of a Region Operation involving one or more Regions

NB modelled as a class. Regions are identifiable, not to be thought of as value types. 
Someone creates a region on purpose, they do not 'just exist'."
	{

		Area : regionAreaType /* utype=stc:regionAreaType*/ @? "" ;
		note : string /* utype=ivoa:string*/ @? "" ;
		coord_system_id references coordSysType /* utype=stc:coordSysType*/ ""
	}

	otype boxType extends shapeType /* utype=stc:shapeType*/
	"Box shape: a rectangle defined by its center and size on both dimensions; since it is a polygon, it is redundant, but simple rectangles with great circle sides are awkward to define in spherical coordinates"
	{

		Center : double2Type /* utype=stc:double2Type*/ @? "" ;
		Size : double2Type /* utype=stc:double2Type*/ @? "" ;
	}

	otype ellipseType extends shapeType /* utype=stc:shapeType*/
	"Ellipse shape: center, semi-major, semi-minor axis and position angle; in spherical coordinates defined as the shape cut out of the sphere by a cone with elliptical cross-section"
	{

		Center : double2Type /* utype=stc:double2Type*/ @? "The coordinates of the ellipse's center" ;
		SemiMajorAxis : double1Type /* utype=stc:double1Type*/ @? "" ;
		SemiMinorAxis : double1Type /* utype=stc:double1Type*/ @? "Half the minor axis of the ellipse, in radius_unit" ;
		PosAngle : posAngleType /* utype=stc:posAngleType*/ @? "Position angle of major axis (Radius)." ;
	}

	abstract otype combinationType extends regionType /* utype=stc:regionType*/ ""
	{

		region : unionRegion /* utype=stc:unionRegion*/ @+ as composition ""
	}

	otype circleType extends shapeType /* utype=stc:shapeType*/ "Circle shape: center and radius"
	{

		center : double2Type /* utype=stc:double2Type*/ @? "The coordinates of the circle's center" ;
		radius : double1Type /* utype=stc:double1Type*/ @? "The radius of the circle" ;
	}

	otype unionType extends combinationType /* utype=stc:combinationType*/ "The union of two or more regions is a region"
	{
	}

	otype negationType extends regionType /* utype=stc:regionType*/ ""
	{
		region references regionType /* utype=stc:regionType*/ ""
	}

	otype unionRegion ""
	{

		CONTAINER : combinationType /* utype=stc:combinationType*/ as composition
		"Reference to container in collection region"
		region references regionType /* utype=stc:regionType*/ ""
	}

	otype polygonType extends shapeType /* utype=stc:shapeType*/
	"Polygon: one or more vertices; counter-clockwise (as seen from 'inside' or from 'top') encircled area is enclosed; sides should span less than 180 deg in each coordinate if spherical; a polygon may not intersect itself"
	{

		Vertex : vertexType /* utype=stc:vertexType*/ @+ as composition
		"In order to form polygons, vertices are to be connected with straight line segments. In the case of spherical coordinates: greatcircle segments; if a smallCircle element si present, the vertex and its predecessor are to be connected with a smallcircle, by default in the CoordSys that is referenced; optionally, a pole may be specified (other than the CoordSys pole) that defines the smallcircle system"
	}

	otype sectorType extends shapeType /* utype=stc:shapeType*/
	"A sector is the counter-clockwise area between two half-lines"
	{

		Position : double2Type /* utype=stc:double2Type*/ @? "" ;
		PosAngle1 : posAngleType /* utype=stc:posAngleType*/ @? "" ;
		PosAngle2 : posAngleType /* utype=stc:posAngleType*/ @? "" ;
	}

	otype vertexType ""
	{

		CONTAINER : polygonType /* utype=stc:polygonType*/ as composition "Reference to container in collection Vertex"
		Position : double2Type /* utype=stc:double2Type*/ @? "" ;
		SmallCircle : smallCircleType /* utype=stc:smallCircleType*/ @? "" ;
	}

	otype intersectionType extends combinationType /* utype=stc:combinationType*/ ""
	{
	}

	otype allSkyType extends shapeType /* utype=stc:shapeType*/ "AllSky type: just a shape without any child elements"
	{
	}

	otype diffType extends regionType /* utype=stc:regionType*/ ""
	{
		Region references regionType /* utype=stc:regionType*/ "" Region2 references regionType /* utype=stc:regionType*/ ""
	}

	otype shapeType extends regionType /* utype=stc:regionType*/
	"is the abstract type that is the building block of regions; in real life it needs to be replaced by a concrete shape"
	{
	}

	otype redshiftFrameType extends coordFrameType /* utype=stc:coordFrameType*/
	"Contains the Doppler definitions, including whether the values are velocity or redshift (value_type)"
	{

		CONTAINER : astroCoordSystemType /* utype=stc:astroCoordSystemType*/ as composition
		"Reference to container in collection RedshiftFrame"
		DopplerDefinition : dopplerDefinitionType /* utype=stc:dopplerDefinitionType*/ @?
		"The Doppler definition used: optical, radio, or pseudo-relativistic (i.e., how is a redshift converted to a velocity); the most common is optical, except when the reference is LSR (usually radio)"
		;
		ReferencePosition : referencePositionType /* utype=stc:referencePositionType*/
		"The Doppler definition used: optical, radio, or pseudo-relativistic (i.e., how is a redshift converted to a velocity); the most common is optical, except when the reference is LSR (usually radio)</"
		;
		value_type : redshiftFrameValue /* utype=stc:redshiftFrameValue*/ @? "" ;
	}

	otype geodType extends icrsType /* utype=stc:icrsType*/
	"The Geodetic reference frame; semi-major axis and inverse flattening may be provided to define the reference spheroid; the default is the IAU 1976 reference spheroid"
	{

		radius : real /* utype=ivoa:real*/ @? "" ;
		inv_flattening : real /* utype=ivoa:real*/ @? "" ;
		unit : posUnitType /* utype=stc:posUnitType*/ @? "" ;
	}

	otype pixelFrameType extends genericCoordinateFrameType /* utype=stc:genericCoordinateFrameType*/ ""
	{

		CONTAINER : pixelCoordSystemType /* utype=stc:pixelCoordSystemType*/ as composition
		"Reference to container in collection CoordFrame"
		ReferencePixel : coordinateType /* utype=stc:coordinateType*/ @? "" ;
		axis1_order : integer /* utype=ivoa:integer*/ "" ;
		axis2_order : integer /* utype=ivoa:integer*/ @? "" ;
		axis3_order : integer /* utype=ivoa:integer*/ @? "" ;
	}

	otype spectralFrameType extends coordFrameType /* utype=stc:coordFrameType*/
	"Contains the spectral frame reference position"
	{

		CONTAINER : astroCoordSystemType /* utype=stc:astroCoordSystemType*/ as composition
		"Reference to container in collection SpectralFrame"
		ReferencePosition : referencePositionType /* utype=stc:referencePositionType*/ @?
		"The reference frame for the spectral scale; note presence of LSR" ;
	}

	otype cart3DRefFrameType extends coordRefFrameType /* utype=stc:coordRefFrameType*/ "A custom space reference frame type defined through a 3-D Cartesian mapping (rotate and scale).
Define coordinate reference frame from scratch;basically a transformation from a known coordinate frame"
	{

		CTransform3 : size3DType /* utype=stc:size3DType*/ @? "NB use the size2DType to represent the various substitutiongroups that are possible for this attribute/.
see also elsewhere" ;
		projection : projectionType /* utype=stc:projectionType*/ @? "" ;
	}

	otype timeFrameType extends coordFrameType /* utype=stc:coordFrameType*/ ""
	{

		CONTAINER : astroCoordSystemType /* utype=stc:astroCoordSystemType*/ as composition
		"Reference to container in collection TimeFrame"
		TimeScale : timeScaleType /* utype=stc:timeScaleType*/ @?
		"The time reference frame consists of a time scale, a time format, and a reference time, if needed" ;
		ReferencePosition : referencePositionType /* utype=stc:referencePositionType*/ @?
		"Origin of the coordinate reference frame: either a 'known place' such as geocenter or barycenter, or a position defined in a known coordinate system"
		;
		TimeRefDirection : astroCoordsType /* utype=stc:astroCoordsType*/ @?
		"Some time transformations (e.g., change of RefPos) depend on an assumed directonal position of the source" ;
	}

	otype spaceRefFrameType extends coordRefFrameType /* utype=stc:coordRefFrameType*/
	"Abstract space reference frame type"
	{

		SpaceRefFrame : SpaceRefFrame /* utype=stc:SpaceRefFrame*/ @? "" ;
	}

	otype coordRefFrameType "A generic CoordReferenceFrame"
	{

		name : string /* utype=ivoa:string*/ @? "" ;
	}

	otype cart2DRefFrameType extends coordRefFrameType /* utype=stc:coordRefFrameType*/ "A custom space reference frame type defined through a 2-D Cartesian mapping (rotate and scale).
Define coordinate reference frame from scratch;Define coordinate reference frame from scratch;basically a transformation from a known coordinate frame"
	{

		CTransform2 : size2DType /* utype=stc:size2DType*/ @? "NB use the size2DType to represent the various substitutiongroups that are possible for this attribute/.
see also elsewhere" ;
		projection : projectionType /* utype=stc:projectionType*/ @? "" ;
	}

	otype icrsType extends spaceRefFrameType /* utype=stc:spaceRefFrameType*/ "ICRS type: no equinox"
	{
	}

	otype observatoryLocationType "Type for an observatory location"
	{

		AstroCoordSystem : obsLocCoordSystem /* utype=stc:obsLocCoordSystem*/ @* as composition ""
	}

	otype sphericalRefFrameType extends coordRefFrameType /* utype=stc:coordRefFrameType*/ "custom space reference frame type defined through pole and X-axis directions
Define coordinate reference frame from scratch; pole and X-axis need to be defined in a known coordinate system

NB did not add ID attribute as this is a Class which carries an implicit identity."
	{

		Frame : string /* utype=ivoa:string*/ "" ;
		Pole_Zaxis : astroCoordsType /* utype=stc:astroCoordsType*/ @? "" ;
		Xaxis : astroCoordsType /* utype=stc:astroCoordsType*/ @? "" ;
	}

	otype genericCoordinateFrameType extends coordFrameType /* utype=stc:coordFrameType*/ ""
	{

		CONTAINER : coordSysType /* utype=stc:coordSysType*/ as composition "Reference to container in collection CoordFrame"
		CoordRefPos : customRefPosType /* utype=stc:customRefPosType*/ @? "" ;
		CoordFlavor : coordFlavorType /* utype=stc:coordFlavorType*/ @?
		"Provides the coordinate definitions: number of axes, SPHERICAL, CARTESIAN, UNITSPHERE, POLAR, or HEALPIX, presence of velocities"
		;
		CoordRefFrame references coordRefFrameType /* utype=stc:coordRefFrameType*/ ""
	}

	otype pixelCoordSystemType extends coordSysType /* utype=stc:coordSysType*/ "The pixel coordinate system definition

NB Mapped the sequence with maxOccurs unbounded to a collection with multiplicity 0..*"
	{

		CoordFrame : pixelFrameType /* utype=stc:pixelFrameType*/ @* as composition ""
	}

	otype coordSysType "Coordinate system definition: a collection of coordinate frames

TBD
Should a coordsys aggregate rather than contain frames?"
	{

		STCReference : STCReference /* utype=stc:STCReference*/ "" ;
		CoordFrame : genericCoordinateFrameType /* utype=stc:genericCoordinateFrameType*/ @* as composition
		"Mapping of the CoordFrame element ref and the type of that global element."
	}

	otype coordFrameType "A CoordFrame has to have at least an Id"
	{

		STCReference : STCReference /* utype=stc:STCReference*/ "" ;
		name : string /* utype=ivoa:string*/ @? "" ;
	}

	otype cart1DRefFrameType extends coordRefFrameType /* utype=stc:coordRefFrameType*/ "A custom space reference frame type defined through a 2-D Cartesian mapping (rotate and scale).
Define coordinate reference frame from scratch;basically a 1-D scaling"
	{

		Scale : double1Type /* utype=stc:double1Type*/ @? "" ;
		projection : projectionType /* utype=stc:projectionType*/ @? "" ;
	}

	otype fkType extends spaceRefFrameType /* utype=stc:spaceRefFrameType*/ "FK[45] type: needs an equinox"
	{

		Equinox : coordEquinoxType /* utype=stc:coordEquinoxType*/ @? "" ;
	}

	otype astroCoordSystemType extends coordSysType /* utype=stc:coordSysType*/
	"The astronomical coordinate system definition: spatial coordinate frame and reference position; time frame and reference position; the coordinate flavor; spectral frame and (optionally) Doppler frame; and the planetary ephemeris; an ID is required, since this is how coordinate elements are associated with their coordinate systems"
	{

		TimeFrame : timeFrameType /* utype=stc:timeFrameType*/ @? as composition ""
		SpaceFrame : spaceFrameType /* utype=stc:spaceFrameType*/ @? as composition ""
		SpectralFrame : spectralFrameType /* utype=stc:spectralFrameType*/ @? as composition ""
		RedshiftFrame : redshiftFrameType /* utype=stc:redshiftFrameType*/ @? as composition ""
	}

	otype obsLocCoordSystem ""
	{

		CONTAINER : observatoryLocationType /* utype=stc:observatoryLocationType*/ as composition
		"Reference to container in collection AstroCoordSystem"
		coordSys references astroCoordSystemType /* utype=stc:astroCoordSystemType*/ ""
	}

	otype spaceFrameType extends coordFrameType /* utype=stc:coordFrameType*/ ""
	{

		CONTAINER : astroCoordSystemType /* utype=stc:astroCoordSystemType*/ as composition
		"Reference to container in collection SpaceFrame"
		ReferencePosition : referencePositionType /* utype=stc:referencePositionType*/ @? "Mapping of the element ref to the substitutiongroup ReferencePosition.
Currently includes the genericRefPosType which is not included in substitution groups." ;
		OffsetCenter : coordValueType /* utype=stc:coordValueType*/ @? "" ;
		CoordFlavor : coordFlavorType /* utype=stc:coordFlavorType*/
		"Provides the coordinate definitions: number of axes, SPHERICAL, CARTESIAN, UNITSPHERE, POLAR, or HEALPIX, presence of velocities"
		;
		SpaceRefFrame references spaceRefFrameType /* utype=stc:spaceRefFrameType*/ "Mapping of contained element of ref SpaceRefFrame substitution group.

Coordinate reference frame: optional equinox with either a standard reference system (ICRS, FK5, FK4) and optional standard pole (equatorial, ecliptic, galactic, etc.), or pole (positive Z-axis) and positive X-axis direction "
	}

	abstract dtype stcBaseType "STC standard base type. Elements of derived types may contain real content, be a reference to another element in the document (idref), or be a reference to a distributed resource; the href is to be a standard IVOA identifier or a valid URL. Actuation is up to the client. At this time it is required to be of type simple. An id is optional; it allows referencing by other elements.

TBD Should this be an objectType?"
	{

		STCReference : STCReference /* utype=stc:STCReference*/ "Mapping of the attribute ref to STCReference." ;
	}

	dtype STCReference "These four attributes represent the standard IVOA referencing system: internal (within the document) referencing through 'id' and 'idref', external referencing through Xlink, using only 'type=simple' and 'href'.These four attributes represent the standard IVOA referencing system: internal (within the document) referencing through 'id' and 'idref', external referencing through Xlink, using only 'type=simple' and 'href'.

The attributes ID_type and IDREF_type allow the author to list attributes of type ID, respectively IDREF, enabling readers to identify these attributes without having to refer to the schema. This is important in cases where documents are concatenated (e.g., in registries), raising the potential for non-unique IDs. The value of these attributes is a string of comma-separated attribute names."
	{

		id : string /* utype=ivoa:string*/ @? "" ;
		idref : string /* utype=ivoa:string*/ @? "" ;
		ucd : string /* utype=ivoa:string*/ @? "" ;
		href : anyURI /* utype=ivoa:anyURI*/ @?
		"Mapping of the xlink:href attribute. note, xlkink:type is not mapped as it is a constant, 'simple'." ;
		ID_TYPE : string /* utype=ivoa:string*/ @? "" ;
		IDREF_type : string /* utype=ivoa:string*/ @? "" ;
	}

	dtype coordValueType "A type that just holds a 1-3D coordinate value; see comment in the CoordValue head element.

NB
From STC schema it seems as if every type of value"
	{

		customValue : string /* utype=ivoa:string*/ "Using string for anyType" ;
		value1D : double1Type /* utype=stc:double1Type*/ @? "" ;
		value2D : double2Type /* utype=stc:double2Type*/ @? "" ;
		value3D : double3Type /* utype=stc:double3Type*/ @? "" ;
	}

	dtype timeOffsetType "Actual elapsed time offset"
	{

		value : decimal /* utype=ivoa:decimal*/ "" ;
		STCReference : STCReference /* utype=stc:STCReference*/ @? "" ;
		unit : timeUnitType /* utype=stc:timeUnitType*/ @? "" ;
	}

	dtype isoTimeType extends AbsoluteTimeType /* utype=stc:AbsoluteTimeType*/ "ISO8601 time; note: only a limited subset of ISO 8601 is allowed: yyyy-mm-ddThh:mm:ss.sss...; unfortunately, XSchema does not allow hh, mm, or ss to be optional, '.ss' is

"
	{

		value : datetime /* utype=ivoa:datetime*/ "" ;
		STCReference : STCReference /* utype=stc:STCReference*/ @? "" ;
	}

	dtype astronTimeType "astronTime is the generalized astronomical time type and consists of one, two, or three elements: optional TimeScale, optional relative time offset, and an absolute time (ISO8601 or a decimal JD or MJD; or it may be an IDREF to one of those three); TimeScale may be omitted only if the element is part of AstroCoords, referring to an AstroCoordSystem that specifies a TimeScale

TBD I suggest this type is to be used as a pattern rather than a reusable type."
	{

		Timescale : timeScaleType /* utype=stc:timeScaleType*/ @? "" ;
		TimeOffset : timeOffsetType /* utype=stc:timeOffsetType*/ @? "" ;
		AbsoluteTime : AbsoluteTimeType /* utype=stc:AbsoluteTimeType*/ "" ;
	}

	dtype relocatableOriginType extends AbsoluteTimeType /* utype=stc:AbsoluteTimeType*/ ""
	{

		value : relocatableOriginTime /* utype=stc:relocatableOriginTime*/ "" ;
	}

	abstract dtype AbsoluteTimeType ""
	{
	}

	dtype jdTimeType extends AbsoluteTimeType /* utype=stc:AbsoluteTimeType*/
	"A decimal type for JD and MJD, with optional referencing"
	{

		value : decimal /* utype=ivoa:decimal*/ "" ;
		STCReference : STCReference /* utype=stc:STCReference*/ @? "" ;
	}

	dtype regionAreaType
	"Element to hold the area of a Region, once calculated; the element holds the actual area, linearAreaUnit the linear units of the of the area (i.e., it should be squared to get the proper units of the area), and validArea indicates whether the area has been calculated properly"
	{

		area : real /* utype=ivoa:real*/ "" ;
		linearAreaUnit : posUnitType /* utype=stc:posUnitType*/ "" ;
		validArea : boolean /* utype=ivoa:boolean*/ "" ;
	}

	dtype smallCircleType ""
	{

		Pole : double2Type /* utype=stc:double2Type*/ @?
		"smallCircleType indicates in polygons that side is along small circle; with optional pole" ;
	}

	dtype double1Type "A double with referencing and units attributes

Mapping of the double1Type.
NB can also be used as type for attributes defined from a ref to root elements of this type. E.g. Value."
	{

		value : real /* utype=ivoa:real*/ "Mapping of the extension of this type of the xsd:double.
We do not inherit from primitives, instead add a value attribute of the corresponding type." ;
		timeUnit : timeUnitType /* utype=stc:timeUnitType*/ @? "" ;
		pos_unit : posUnitType /* utype=stc:posUnitType*/ @? "" ;
		pos_angle_unit : angleUnitType /* utype=stc:angleUnitType*/ @? "" ;
		vel_time_unit : velTimeUnitType /* utype=stc:velTimeUnitType*/ @? "" ;
		spectral_unit : spectralUnitType /* utype=stc:spectralUnitType*/ @? "" ;
		gen_unit : string /* utype=ivoa:string*/ @? "" ;
	}

